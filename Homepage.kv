#:kivy 1.0

MDScreen:
    md_bg_color: [60/255, 140/255, 255/255, 1]

<homepage>
    name: "home"
    BoxLayout:
        orientation: "vertical"
        # size_hint:(1,1)
        Label:
            size_hint:(.1,.1)
            text: "hey"
            
        MDCard:
            # size_hint:(1,1)
            # pos_hint: {"center_x": 0.50, "y": 0}
            
            ScrollView:
                orientation: "vertical"
                size_hint_y: .99
                size_hint_x: .99
                pos_hint: {'x':.03, 'y':.001}
                do_scroll_x:False
                do_scroll_y:True
                
             
                BoxLayout:   
                    orientation:"vertical"
                    # size_hint:(.9,.9)
                    size_hint_y: None
                    size: (root.width, root.height)
                    height: self.minimum_height
                    

                    Label:
                        pos_hint: {"center_x": .3, "center_y": 0.02}
                        size_hint:(1,.3)
                        text: "Housing Analysis"
                        md_bg_color: [.3, 0, 0, 1]
                
                    # color: "Gray"
                    MDCard:
                        MDCarousel:
                            id: pic
                            # size_hint:(1,1)
                            direction: 'left'
                            AsyncImage:
                                source: 'house.jpg'
                                # AsyncImage:
                                #     source: 'bg.png'
                        
                    ScrollView:
                        orientation: "vertical"
                        size_hint_y: .99
                        size_hint_x: .99
                        pos_hint: {'x':.03, 'y':.001}
                        do_scroll_x:False
                        do_scroll_y:True
                        
